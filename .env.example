# Puerto del servidor (Vercel lo configura automáticamente)
PORT=5001

# Base de datos PostgreSQL
DATABASE_URL=postgresql://user:password@host:5432/database

# Configuración de Firebase
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# ID de la empresa
VITE_EMPRESA_ID=your_empresa_id

# URLs de Firebase Functions
VITE_FIREBASE_FUNCTION_URL=https://us-central1-your-project.cloudfunctions.net/register
VITE_EMAIL_API_URL=https://us-central1-your-project.cloudfunctions.net/emailAPI/sendEmail

# URL del landing
VITE_LANDING_URL=https://your-app.vercel.app
